{
  "id": null,
  "title": "OPS - Infrastructure & Traffic Deep Dive",
  "tags": ["operations", "infrastructure", "traffic", "deep-dive", "containers"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "30s",
  "time": {
    "from": "now-3h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "ðŸš€ Infrastructure Deep Dive",
      "type": "text",
      "gridPos": {"h": 3, "w": 24, "x": 0, "y": 0},
      "options": {
        "mode": "markdown",
        "content": "# Infrastructure & Traffic Deep Dive Dashboard\n\n**Audience**: Operations teams and site reliability engineers\n**Purpose**: Detailed monitoring of container resources, edge router performance, and traffic patterns for comprehensive infrastructure visibility\n\nðŸŽ¯ **Focus Areas**: Container metrics, Traefik performance, database operations, and resource utilization trends"
      }
    },
    {
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 3},
      "id": 2,
      "panels": [],
      "title": "Traefik Edge Router Performance",
      "type": "row"
    },
    {
      "id": 3,
      "title": "Traefik Request Distribution",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "targets": [
        {
          "expr": "sum(rate(traefik_service_requests_total{service=\"api-service@file\"}[5m])) * 60 or vector(0)",
          "legendFormat": "API Service RPM",
          "refId": "A"
        },
        {
          "expr": "sum(rate(traefik_entrypoint_requests_total[5m])) by (entrypoint) * 60 or vector(0)",
          "legendFormat": "{{entrypoint}} EntryPoint",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "min": 0
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "bottom"}
      }
    },
    {
      "id": 4,
      "title": "Traefik Response Times",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(traefik_service_request_duration_seconds_bucket{service=\"api-service@file\"}[1h])) by (le)) * 1000 or vector(0)",
          "legendFormat": "API Service P95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(traefik_service_request_duration_seconds_bucket{service=\"api-service@file\"}[1h])) by (le)) * 1000 or vector(0)",
          "legendFormat": "API Service P50",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(traefik_entrypoint_request_duration_seconds_bucket[1h])) by (le, entrypoint)) * 1000 or vector(0)",
          "legendFormat": "{{entrypoint}} P95",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "bottom"}
      }
    },
    {
      "id": 5,
      "title": "Traefik Error Rates",
      "type": "stat",
      "gridPos": {"h": 6, "w": 8, "x": 0, "y": 12},
      "targets": [
        {
          "expr": "sum(rate(traefik_service_requests_total{service=\"api-service@file\", code=~\"5..\"}[1h])) / sum(rate(traefik_service_requests_total{service=\"api-service@file\"}[1h])) * 100 or vector(0)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "displayName": "5xx Error Rate",
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "red", "value": 5}
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area"
      }
    },
    {
      "id": 6,
      "title": "Traefik Open Connections",
      "type": "stat",
      "gridPos": {"h": 6, "w": 8, "x": 8, "y": 12},
      "targets": [
        {
          "expr": "sum(traefik_service_open_connections{service=\"api-service@file\"}) or vector(0)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "displayName": "Open Connections",
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "red", "value": 100}
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 7,
      "title": "TLS Connections",
      "type": "stat",
      "gridPos": {"h": 6, "w": 8, "x": 16, "y": 12},
      "targets": [
        {
          "expr": "sum(rate(traefik_service_requests_tls_total{service=\"api-service@file\"}[5m])) * 60 or vector(0)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "displayName": "TLS Requests/min",
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 100},
              {"color": "red", "value": 500}
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 18},
      "id": 8,
      "panels": [],
      "title": "Container Resource Utilization",
      "type": "row"
    },
    {
      "id": 9,
      "title": "Container CPU Usage",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 19},
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{name=\"qr_generator_api\"}[5m])) by (name) * 100 or vector(0)",
          "legendFormat": "API Container CPU %",
          "refId": "A"
        },
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{name=\"qr_generator_postgres\"}[5m])) by (name) * 100 or vector(0)",
          "legendFormat": "PostgreSQL CPU %",
          "refId": "B"
        },
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{name=\"qr_generator_traefik\"}[5m])) by (name) * 100 or vector(0)",
          "legendFormat": "Traefik CPU %",
          "refId": "C"
        },
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{name=\"qr_generator_prometheus\"}[5m])) by (name) * 100 or vector(0)",
          "legendFormat": "Prometheus CPU %",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "bottom"}
      }
    },
    {
      "id": 10,
      "title": "Container Memory Usage",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 19},
      "targets": [
        {
          "expr": "container_memory_usage_bytes{name=\"qr_generator_api\"} / 1024 / 1024 or vector(0)",
          "legendFormat": "API Container (MB)",
          "refId": "A"
        },
        {
          "expr": "container_memory_usage_bytes{name=\"qr_generator_postgres\"} / 1024 / 1024 or vector(0)",
          "legendFormat": "PostgreSQL (MB)",
          "refId": "B"
        },
        {
          "expr": "container_memory_usage_bytes{name=\"qr_generator_traefik\"} / 1024 / 1024 or vector(0)",
          "legendFormat": "Traefik (MB)",
          "refId": "C"
        },
        {
          "expr": "container_memory_usage_bytes{name=\"qr_generator_prometheus\"} / 1024 / 1024 or vector(0)",
          "legendFormat": "Prometheus (MB)",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "decbytes",
          "min": 0
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "bottom"}
      }
    },
    {
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 27},
      "id": 11,
      "panels": [],
      "title": "Database Performance Patterns",
      "type": "row"
    },
    {
      "id": 12,
      "title": "Database Connection Patterns",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
      "targets": [
        {
          "expr": "pg_stat_activity_count{datname=\"qrdb\", state=\"active\"} or vector(0)",
          "legendFormat": "Active Connections",
          "refId": "A"
        },
        {
          "expr": "pg_stat_activity_count{datname=\"qrdb\", state=\"idle\"} or vector(0)",
          "legendFormat": "Idle Connections",
          "refId": "B"
        },
        {
          "expr": "pg_stat_activity_count{datname=\"qrdb\", state=\"idle in transaction\"} or vector(0)",
          "legendFormat": "Idle in Transaction",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "bottom"}
      }
    },
    {
      "id": 13,
      "title": "Database Performance Metrics",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
      "targets": [
        {
          "expr": "sum(rate(pg_stat_database_xact_commit{datname=\"qrdb\"}[5m]) + rate(pg_stat_database_xact_rollback{datname=\"qrdb\"}[5m])) or vector(0)",
          "legendFormat": "Transactions/sec",
          "refId": "A"
        },
        {
          "expr": "rate(pg_stat_database_tup_inserted{datname=\"qrdb\"}[5m]) or vector(0)",
          "legendFormat": "Inserts/sec",
          "refId": "B"
        },
        {
          "expr": "rate(pg_stat_database_tup_updated{datname=\"qrdb\"}[5m]) or vector(0)",
          "legendFormat": "Updates/sec",
          "refId": "C"
        },
        {
          "expr": "rate(pg_stat_database_tup_deleted{datname=\"qrdb\"}[5m]) or vector(0)",
          "legendFormat": "Deletes/sec",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "min": 0
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "bottom"}
      }
    },
    {
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 36},
      "id": 14,
      "panels": [],
      "title": "Application Runtime Metrics",
      "type": "row"
    },
    {
      "id": 15,
      "title": "Application Go Runtime Metrics",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 37},
      "targets": [
        {
          "expr": "go_memstats_alloc_bytes{job=\"qr-app\"} / 1024 / 1024 or vector(0)",
          "legendFormat": "Allocated Memory (MB)",
          "refId": "A"
        },
        {
          "expr": "go_memstats_sys_bytes{job=\"qr-app\"} / 1024 / 1024 or vector(0)",
          "legendFormat": "System Memory (MB)",
          "refId": "B"
        },
        {
          "expr": "go_goroutines{job=\"qr-app\"} or vector(0)",
          "legendFormat": "Goroutines",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "bottom"}
      }
    },
    {
      "id": 16,
      "title": "Garbage Collection Metrics",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 37},
      "targets": [
        {
          "expr": "rate(go_gc_duration_seconds_sum{job=\"qr-app\"}[5m]) * 1000 or vector(0)",
          "legendFormat": "GC Duration (ms/sec)",
          "refId": "A"
        },
        {
          "expr": "rate(go_gc_duration_seconds_count{job=\"qr-app\"}[5m]) or vector(0)",
          "legendFormat": "GC Frequency (GCs/sec)",
          "refId": "B"
        },
        {
          "expr": "go_memstats_gc_cpu_fraction{job=\"qr-app\"} * 100 or vector(0)",
          "legendFormat": "GC CPU Usage %",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "bottom"}
      }
    },
    {
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 45},
      "id": 17,
      "panels": [],
      "title": "Log Analysis & Error Tracking",
      "type": "row"
    },
    {
      "id": 20,
      "title": "Application Error Log Rate",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 46},
      "targets": [
        {
          "expr": "sum(rate({job=\"qr-app\"} |= \"ERROR\" [5m])) or vector(0)",
          "legendFormat": "ERROR Log Rate",
          "refId": "A"
        },
        {
          "expr": "sum(rate({job=\"qr-app\"} |= \"WARNING\" [5m])) or vector(0)",
          "legendFormat": "WARNING Log Rate",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "min": 0
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "bottom"}
      }
    },
    {
      "id": 21,
      "title": "Recent Error Logs",
      "type": "logs",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 46},
      "targets": [
        {
          "expr": "{job=\"qr-app\"} |= \"ERROR\" | line_format \"{{.timestamp}} [{{.level}}] {{.message}}\"",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "dedupStrategy": "none",
        "sortOrder": "Descending"
      }
    },
    {
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 54},
      "id": 22,
      "panels": [],
      "title": "Network & I/O Performance",
      "type": "row"
    },
    {
      "id": 18,
      "title": "Container Network I/O",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 55},
      "targets": [
        {
          "expr": "rate(container_network_receive_bytes_total{name=\"qr_generator_api\"}[5m]) * 8 or vector(0)",
          "legendFormat": "API RX (bits/sec)",
          "refId": "A"
        },
        {
          "expr": "rate(container_network_transmit_bytes_total{name=\"qr_generator_api\"}[5m]) * 8 or vector(0)",
          "legendFormat": "API TX (bits/sec)",
          "refId": "B"
        },
        {
          "expr": "rate(container_network_receive_bytes_total{name=\"qr_generator_postgres\"}[5m]) * 8 or vector(0)",
          "legendFormat": "PostgreSQL RX (bits/sec)",
          "refId": "C"
        },
        {
          "expr": "rate(container_network_transmit_bytes_total{name=\"qr_generator_postgres\"}[5m]) * 8 or vector(0)",
          "legendFormat": "PostgreSQL TX (bits/sec)",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bps",
          "min": 0
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "bottom"}
      }
    },
    {
      "id": 19,
      "title": "System Resource Summary",
      "type": "table",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 55},
      "targets": [
        {
          "expr": "up{job=~\"prometheus|qr-app|traefik\"} or vector(0)",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "displayMode": "basic"
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Value"},
            "properties": [
              {"id": "displayName", "value": "Status"},
              {"id": "unit", "value": "short"},
              {"id": "custom.displayMode", "value": "color-background"},
              {"id": "thresholds", "value": {
                "steps": [
                  {"color": "red", "value": null},
                  {"color": "green", "value": 1}
                ]
              }}
            ]
          }
        ]
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "__name__": true,
              "Time": true
            },
            "indexByName": {},
            "renameByName": {}
          }
        }
      ]
    }
